{
  "name": "@iamnnort/config",
  "version": "1.1.8",
  "description": "Config presets for Node.js - Consistent - Reusable - Streamlined",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamnnort/config.git"
  },
  "bugs": "https://github.com/iamnnort/config/issues",
  "homepage": "https://github.com/iamnnort/config#readme",
  "keywords": [
    "config",
    "eslint",
    "prettier",
    "lint-staged",
    "esbuild",
    "tsup",
    "typescript",
    "tsconfig",
    "http"
  ],
  "files": [
    "dist",
    "tsconfig"
  ],
  "exports": {
    "./esbuild": {
      "types": "./dist/esbuild/index.d.ts",
      "default": "./dist/esbuild/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.ts",
      "default": "./dist/eslint/index.js"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "default": "./dist/http/index.js"
    },
    "./jest": {
      "types": "./dist/jest/index.d.ts",
      "default": "./dist/jest/index.js"
    },
    "./lint-staged": {
      "types": "./dist/lint-staged/index.d.ts",
      "default": "./dist/lint-staged/index.js"
    },
    "./prettier": {
      "types": "./dist/prettier/index.d.ts",
      "default": "./dist/prettier/index.js"
    },
    "./tsup": {
      "types": "./dist/tsup/index.d.ts",
      "default": "./dist/tsup/index.js"
    },
    "./tsconfig/node.json": {
      "default": "./tsconfig/node.json"
    },
    "./tsconfig/nest.json": {
      "default": "./tsconfig/nest.json"
    },
    "./tsconfig/browser.json": {
      "default": "./tsconfig/browser.json"
    }
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup",
    "deploy": "npm publish",
    "lint": "eslint --fix",
    "format": "prettier --write . --ignore-path .gitignore",
    "test": "yarn lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@tsconfig/node22": "^22.0.5",
    "@typescript-eslint/parser": "^8.53.1",
    "axios": "^1.13.5",
    "dotenv": "^17.2.3",
    "esbuild-plugin-tsc": "^0.5.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "tsup": "^8.4.0",
    "typescript-eslint": "^8.53.1"
  },
  "devDependencies": {
    "@swc/core": "^1.15.11",
    "@types/node": "^25.0.10",
    "esbuild": "^0.25.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
